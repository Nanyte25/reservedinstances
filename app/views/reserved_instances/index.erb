<h1>Reserved Instances</h1>

<table class="table table-striped table-hover" id="data_table" data-pagination="true" data-search="true" <%= "data-search-text=" + params[:search] if !params[:search].blank?%>>
  <thead>
    <tr>
      <th>Account ID</th>
      <th>Reservation ID</th>
      <th>Instance type</th>
      <th>Availability Zone</th>
      <th>Tenancy</th>
      <th>Platform</th>
      <th>Network</th>
      <th>Count</th>
      <th>End Date</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    <% @reserved_instances.each do |ri| %>
      <tr>
        <td><%= ri.accountid %></td>
        <td><%= ri.reservationid %></td>
        <td><%= ri.instancetype %></td>
        <td><%= ri.az %></td>
        <td><%= ri.tenancy %></td>
        <td><%= ri.platform %></td>
        <td><%= ri.network %></td>
        <td><%= ri.count %></td>
        <td><%= ri.enddate %></td>
        <td><%= ri.status %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<script type="text/javascript">
$(function () {
    $('#data_table').bootstrapTable({showExport: true});
            });
</script>
